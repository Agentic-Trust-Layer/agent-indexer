{
  "name": "agentic-trust-layer",
  "license": "UNLICENSED",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "render:mainnet": "node scripts/render-subgraph.cjs mainnet",
    "render:sepolia": "node scripts/render-subgraph.cjs sepolia",
    "render:linea": "node scripts/render-subgraph.cjs linea",
    "render:linea-sepolia": "node scripts/render-subgraph.cjs linea-sepolia",
    "codegen:mainnet": "node scripts/render-subgraph.cjs mainnet && graph codegen subgraph.mainnet.yaml",
    "build:mainnet": "node scripts/render-subgraph.cjs mainnet && graph build subgraph.mainnet.yaml",
    "codegen:sepolia": "node scripts/render-subgraph.cjs sepolia && graph codegen subgraph.sepolia.yaml",
    "build:sepolia": "node scripts/render-subgraph.cjs sepolia && graph build subgraph.sepolia.yaml",
    "codegen:linea": "node scripts/render-subgraph.cjs linea && graph codegen subgraph.linea.yaml",
    "build:linea": "node scripts/render-subgraph.cjs linea && graph build subgraph.linea.yaml",
    "codegen:linea-sepolia": "node scripts/render-subgraph.cjs linea-sepolia && graph codegen subgraph.linea-sepolia.yaml",
    "build:linea-sepolia": "node scripts/render-subgraph.cjs linea-sepolia && graph build subgraph.linea-sepolia.yaml",
    "codegen:base": "graph codegen subgraph.base-sepolia.yaml",
    "build:base": "graph build subgraph.base-sepolia.yaml",
    "codegen:l2": "graph codegen --output-dir ./generatedL2 subgraphL2.yaml",
    "build:l2": "graph build --output-dir ./buildL2 subgraphL2.yaml",
    "codegen:basesepolia": "graph codegen --output-dir ./generatedBaseSepolia subgraph.basesepolia.yaml",
    "build:basesepolia": "graph build --output-dir ./buildBaseSepolia subgraph.basesepolia.yaml",
    "codegen:basemainnet": "graph codegen --output-dir ./generatedBaseMainnet subgraph.basemainnet.yaml",
    "build:basemainnet": "graph build --output-dir ./buildBaseMainnet subgraph.basemainnet.yaml",
    "deploy": "graph deploy --node https://api.studio.thegraph.com/deploy/ agentic-trust-layer",
    "deploy:l2": "graph deploy --studio agentic-trust-layer-l2 --output-dir ./buildL2 subgraphL2.yaml",
    "deploy:l2:thegraph": "graph deploy --node https://api.studio.thegraph.com/deploy/ agentic-trust-layer-l2 --output-dir ./buildL2 subgraphL2.yaml",
    "deploy:base:p2p": "graph deploy --node https://api.graph-ams.p2pify.com/3fee37a41850472fa3e9ea674598e6b9/deploy --ipfs https://api.graph-ams.p2pify.com/3fee37a41850472fa3e9ea674598e6b9/ipfs agentic-trust-layer-base subgraph.base-sepolia.yaml",
    "codegen:eas": "graph codegen --output-dir ./generatedEas subgraphEas.yaml",
    "build:eas": "graph build --output-dir ./buildEas subgraphEas.yaml",
    "deploy:eas": "graph deploy --studio agentic-trust-layer-eas --output-dir ./buildEas subgraphEas.yaml",
    "create-local": "graph create --node http://localhost:8020/ agentic-trust-layer",
    "remove-local": "graph remove --node http://localhost:8020/ agentic-trust-layer",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 agentic-trust-layer",
    "test": "graph test"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.98.1",
    "@graphprotocol/graph-ts": "0.37.0"
  },
  "devDependencies": { "matchstick-as": "0.6.0" }
}
